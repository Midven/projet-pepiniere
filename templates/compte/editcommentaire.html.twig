{% extends "base.html.twig" %}

{% block title %}Modification du commentaire
{% endblock %}

{% block body %}

	<div class="container">
		<h1 class="mt-3 mb-3">Modification du commentaire</h1>

		{{ form_start(form) }}

		{{ form_row(form.note) }}
		{{ form_row(form.contenu) }}

		<button class="btn btn-success mt-3 mb-3">Editez votre commentaire</button>
		{{ form_end(form) }}

	</div>

{% endblock %}
